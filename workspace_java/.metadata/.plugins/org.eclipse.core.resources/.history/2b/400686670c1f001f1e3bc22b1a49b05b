package chap05;

public class Array문제Exam {

	public static void main(String[] args) {
		int[][] array = {
				{95,86},
				{83,92,96},
				{78,83,93,87,88},
		};
		
		
		for(int i= 0; i< array.length; i++) {
			for(int j=0; j< array[i][j].length;j++) {
				System.out.println("["+i+"]"+"["+j+"]");
			}
		}
		System.out.println(array);

	}

}
