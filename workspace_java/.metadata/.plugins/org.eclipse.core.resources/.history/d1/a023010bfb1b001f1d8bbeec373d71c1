package sec01.exam01;

public class ForExam {

	public static void main(String[] args) {
		
		// 1씩 증가하는걸 5번 할꺼임
		int sum = 0;
		sum = sum +1;
//		sum += 1;
//		sum++;
		sum = sum +1;
		sum = sum +1;
		sum = sum +1;
		sum = sum +1;
		System.out.println(sum);
	
		// 1부터 5를 더할 꺼임
		
		sum = 0;
		sum = sum +1;
		sum = sum +2;
		sum = sum +3;
		sum = sum +4;
		sum = sum +5;
		System.out.println(sum);
	
	}

}
